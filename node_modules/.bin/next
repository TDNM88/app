#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/app/node_modules/.pnpm/next@10.2.3_react-dom@17.0.2_react@17.0.2/node_modules/next/dist/bin/node_modules:/workspaces/app/node_modules/.pnpm/next@10.2.3_react-dom@17.0.2_react@17.0.2/node_modules/next/dist/node_modules:/workspaces/app/node_modules/.pnpm/next@10.2.3_react-dom@17.0.2_react@17.0.2/node_modules/next/node_modules:/workspaces/app/node_modules/.pnpm/next@10.2.3_react-dom@17.0.2_react@17.0.2/node_modules:/workspaces/app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/app/node_modules/.pnpm/next@10.2.3_react-dom@17.0.2_react@17.0.2/node_modules/next/dist/bin/node_modules:/workspaces/app/node_modules/.pnpm/next@10.2.3_react-dom@17.0.2_react@17.0.2/node_modules/next/dist/node_modules:/workspaces/app/node_modules/.pnpm/next@10.2.3_react-dom@17.0.2_react@17.0.2/node_modules/next/node_modules:/workspaces/app/node_modules/.pnpm/next@10.2.3_react-dom@17.0.2_react@17.0.2/node_modules:/workspaces/app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../next/dist/bin/next" "$@"
else
  exec node  "$basedir/../next/dist/bin/next" "$@"
fi
